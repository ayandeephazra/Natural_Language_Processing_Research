                                                                        </mml:mrow>
                                                                        <mml:mrow>
                                                                           <mml:mi>V</mml:mi>
                                                                           <mml:msup>
                                                                              <mml:mrow>
                                                                                 <mml:mi>o</mml:mi>
                                                                              </mml:mrow>
                                                                              <mml:mrow>
                                                                                 <mml:mn>2</mml:mn>
                                                                                 <mml:mo linebreak="badbreak">+</mml:mo>
                                                                              </mml:mrow>
                                                                           </mml:msup>
                                                                        </mml:mrow>
                                                                        <mml:mrow>
                                                                           <mml:mi>i</mml:mi>
                                                                           <mml:mi>n</mml:mi>
                                                                        </mml:mrow>
                                                                     </mml:msubsup>
                                                                     <mml:mo linebreak="badbreak">=</mml:mo>
                                                                     <mml:msub>
                                                                        <mml:mrow>
                                                                           <mml:mi>c</mml:mi>
                                                                        </mml:mrow>
                                                                        <mml:mrow>
                                                                           <mml:mn>0</mml:mn>
                                                                        </mml:mrow>
                                                                     </mml:msub>
                                                                     <mml:mrow>
                                                                        <mml:mo stretchy="true">(</mml:mo>
                                                                        <mml:mn>1</mml:mn>
                                                                        <mml:mo linebreak="badbreak">−</mml:mo>
                                                                        <mml:mi>S</mml:mi>
                                                                        <mml:mi>O</mml:mi>
                                                                        <mml:mi>C</mml:mi>
                                                                        <mml:mo stretchy="true">)</mml:mo>
                                                                     </mml:mrow>
                                                                  </mml:mrow>
                                                               </mml:mtd>
                                                            </mml:mtr>
                                                            <mml:mtr>
                                                               <mml:mtd>
                                                                  <mml:mrow>
                                                                     <mml:msubsup>
                                                                        <mml:mrow>
                                                                           <mml:mi>c</mml:mi>
                                                                        </mml:mrow>
                                                                        <mml:mrow>
                                                                           <mml:mi>V</mml:mi>
                                                                           <mml:msubsup>
                                                                              <mml:mrow>
                                                                                 <mml:mi>O</mml:mi>
                                                                              </mml:mrow>
                                                                              <mml:mrow>
                                                                                 <mml:mn>2</mml:mn>
                                                                              </mml:mrow>
                                                                              <mml:mrow>
                                                                                 <mml:mo>+</mml:mo>
                                                                              </mml:mrow>
                                                                           </mml:msubsup>
                                                                        </mml:mrow>
                                                                        <mml:mrow>
