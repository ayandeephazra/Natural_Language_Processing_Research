            <ce:dochead id="cedoch10">
               <ce:textfn>Review</ce:textfn>
            </ce:dochead>
            <ce:title id="cetitle10">Electrochemical conversion of C1 molecules to sustainable fuels in solid oxide electrolysis cells</ce:title>
            <ce:author-group id="aug10">
               <ce:author id="au10" author-id="S187220672163838X-d01f7f38deb9d190a9c5c212ee9a4f52">
