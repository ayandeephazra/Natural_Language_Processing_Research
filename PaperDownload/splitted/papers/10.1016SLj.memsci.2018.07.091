                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
                           <ce:formula id="eq0055">
                              <ce:label>(11)</ce:label>
                              <mml:math altimg="si0030.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>d</mml:mi>
                                       </mml:mrow>
