                                       <mml:mrow>
                                          <mml:mo>%</mml:mo>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                    <mml:mo>=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mrow>
