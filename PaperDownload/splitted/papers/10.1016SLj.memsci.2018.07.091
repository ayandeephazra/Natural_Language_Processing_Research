                                                   <mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mn>1</mml:mn>
                                                         <mml:mo>+</mml:mo>
                                                         <mml:mi mathvariant="normal">Δ</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>V</mml:mi>
