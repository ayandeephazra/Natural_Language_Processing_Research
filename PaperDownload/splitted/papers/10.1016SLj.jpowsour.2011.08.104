                                          <mml:mi>s</mml:mi>
                                          <mml:mi>y</mml:mi>
                                          <mml:mi>s</mml:mi>
                                          <mml:mi>t</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>m</mml:mi>
                                          <mml:mo>_</mml:mo>
                                          <mml:mi>n</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>=</mml:mo>
                                    <mml:msub>
                                       <mml:mi>P</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>s</mml:mi>
                                          <mml:mi>y</mml:mi>
                                          <mml:mi>s</mml:mi>
                                          <mml:mi>t</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>m</mml:mi>
                                          <mml:mo>_</mml:mo>
                                          <mml:mi>t</mml:mi>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>t</mml:mi>
                                          <mml:mi>a</mml:mi>
                                          <mml:mi>l</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>−</mml:mo>
                                    <mml:msub>
                                       <mml:mi>P</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>s</mml:mi>
                                          <mml:mi>y</mml:mi>
                                          <mml:mi>s</mml:mi>
                                          <mml:mi>t</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>m</mml:mi>
                                          <mml:mo>_</mml:mo>
                                          <mml:mi>c</mml:mi>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>n</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>=</mml:mo>
                                    <mml:mi>A</mml:mi>
                                    <mml:mi>m</mml:mi>
                                    <mml:msub>
                                       <mml:mi>n</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>c</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>l</mml:mi>
                                          <mml:mi>l</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mi>f</mml:mi>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mi>T</mml:mi>
                                    <mml:mo>,</mml:mo>
                                    <mml:msub>
                                       <mml:mi>C</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>M</mml:mi>
                                          <mml:mi>E</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>,</mml:mo>
                                    <mml:msub>
                                       <mml:mi>F</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>M</mml:mi>
                                          <mml:mi>E</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>,</mml:mo>
                                    <mml:msub>
                                       <mml:mi>F</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>A</mml:mi>
                                          <mml:mi>I</mml:mi>
                                          <mml:mi>R</mml:mi>
                                       </mml:mrow>
