                                    <mml:mo stretchy="false">)</mml:mo>
                                    <mml:mi>j</mml:mi>
                                    <mml:mo>−</mml:mo>
                                    <mml:mi>g</mml:mi>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mi>T</mml:mi>
                                    <mml:mo>,</mml:mo>
                                    <mml:msub>
                                       <mml:mi>C</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>M</mml:mi>
                                          <mml:mi>E</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>,</mml:mo>
                                    <mml:msub>
                                       <mml:mi>F</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>M</mml:mi>
                                          <mml:mi>E</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
