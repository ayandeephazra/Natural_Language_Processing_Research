                                    <mml:mo>,</mml:mo>
                                    <mml:msub>
                                       <mml:mi>C</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>M</mml:mi>
                                          <mml:mi>E</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>,</mml:mo>
                                    <mml:msub>
                                       <mml:mi>F</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>M</mml:mi>
                                          <mml:mi>E</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>,</mml:mo>
                                    <mml:msub>
                                       <mml:mi>F</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>A</mml:mi>
                                          <mml:mi>I</mml:mi>
                                          <mml:mi>R</mml:mi>
                                       </mml:mrow>
