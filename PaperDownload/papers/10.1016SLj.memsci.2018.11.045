                                    </mml:msubsup>
                                    <mml:mo>=</mml:mo>
                                    <mml:msubsup>
                                       <mml:mrow>
                                          <mml:mover accent="true">
                                             <mml:mi>μ</mml:mi>
                                             <mml:mo>¯</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
