                                    <mml:msubsup>
                                       <mml:mrow>
                                          <mml:mi>l</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>T</mml:mi>
                                          <mml:mi>P</mml:mi>
                                          <mml:mi>B</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mrow>
                                                <mml:mi>t</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi>n</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo linebreak="goodbreak">=</mml:mo>
                                    <mml:msubsup>
                                       <mml:mrow>
                                          <mml:mi>l</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>T</mml:mi>
