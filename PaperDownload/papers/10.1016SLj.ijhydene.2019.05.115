                                                            <mml:mrow>
                                                               <mml:mi>r</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mn>2</mml:mn>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                         <mml:msub>
                                                            <mml:mrow>
