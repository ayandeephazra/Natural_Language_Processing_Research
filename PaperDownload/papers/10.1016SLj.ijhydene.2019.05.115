                                          <mml:mi>B</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>0</mml:mn>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mrow>
                                       <mml:mo stretchy="true">(</mml:mo>
                                       <mml:mn>1</mml:mn>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mrow>
