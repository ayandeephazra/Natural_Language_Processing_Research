                                          <mml:mtext>Fe</mml:mtext>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>3</mml:mn>
                                          <mml:mo>+</mml:mo>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mo>+</mml:mo>
