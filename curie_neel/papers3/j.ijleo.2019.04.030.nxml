            <ce:title id="tit0005">Manipulation of Bragg and graphene photonic band gaps in one-dimensional photonic crystal containing graphene</ce:title>
            <ce:author-group id="aug0005">
               <ce:author id="aut0005" author-id="S0030402619305005-589a53aa17f8d823770cf252dd429da0">
                  <ce:given-name>V.</ce:given-name>
                  <ce:surname>Pourmahmoud</ce:surname>
               </ce:author>
               <ce:author id="aut0010" orcid="0000-0002-3981-7769" author-id="S0030402619305005-781b4b1dbeabbe5c1519e13f75c0959d">
                  <ce:given-name>B.</ce:given-name>
                  <ce:surname>Rezaei</ce:surname>
