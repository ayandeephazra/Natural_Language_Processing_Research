                                                         <mml:mfenced open="(" close=")" separators=",">
                                                            <mml:mo>exp</mml:mo>
                                                         </mml:mfenced>
                                                      </mml:mrow>
                                                   </mml:msub>
                                                   <mml:mo>−</mml:mo>
