                                 <mml:mi mathvariant="italic">Mean</mml:mi>
                                 <mml:mo>=</mml:mo>
                                 <mml:msqrt>
                                    <mml:mfrac>
                                       <mml:mstyle displaystyle="true">
                                          <mml:munderover>
                                             <mml:mo stretchy="true">∑</mml:mo>
                                             <mml:mrow>
                                                <mml:mi>i</mml:mi>
                                                <mml:mo>=</mml:mo>
