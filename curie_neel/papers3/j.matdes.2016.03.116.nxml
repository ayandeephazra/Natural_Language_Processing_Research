                                 <mml:msqrt>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mn>1</mml:mn>
                                          <mml:mi>n</mml:mi>
                                       </mml:mfrac>
                                       <mml:mstyle displaystyle="true">
                                          <mml:munderover>
                                             <mml:mo stretchy="true">∑</mml:mo>
