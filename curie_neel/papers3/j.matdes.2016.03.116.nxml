                                          </mml:munderover>
                                          <mml:msub>
                                             <mml:mi>E</mml:mi>
                                             <mml:mrow>
                                                <mml:mi mathvariant="italic">gi</mml:mi>
                                                <mml:mfenced open="(" close=")" separators=",">
                                                   <mml:mi mathvariant="italic">est</mml:mi>
                                                </mml:mfenced>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mstyle>
                                       <mml:mi>n</mml:mi>
