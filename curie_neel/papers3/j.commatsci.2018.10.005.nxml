                                       <mml:mi>β</mml:mi>
                                       <mml:mi>γ</mml:mi>
                                    </mml:mrow>
                                 </mml:munder>
                              </mml:mstyle>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi mathvariant="normal">Φ</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>α</mml:mi>
                                    <mml:mi>β</mml:mi>
                                    <mml:mi>γ</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
