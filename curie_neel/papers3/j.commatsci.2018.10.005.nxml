                              <mml:mi>j</mml:mi>
                              <mml:mo>,</mml:mo>
                              <mml:mi>k</mml:mi>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mfrac>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>e</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>α</mml:mi>
                                          <mml:mo>,</mml:mo>
                                          <mml:mi>λ</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mi>i</mml:mi>
                                    <mml:mo stretchy="false">)</mml:mo>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>e</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>β</mml:mi>
                                          <mml:mo>,</mml:mo>
                                          <mml:mo>±</mml:mo>
                                          <mml:msup>
                                             <mml:mrow>
                                                <mml:mi>λ</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mo>′</mml:mo>
                                             </mml:mrow>
                                          </mml:msup>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mi>j</mml:mi>
                                    <mml:mo stretchy="false">)</mml:mo>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>e</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>γ</mml:mi>
                                          <mml:mo>,</mml:mo>
                                          <mml:mo>-</mml:mo>
                                          <mml:msup>
                                             <mml:mrow>
                                                <mml:mi>λ</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mo>″</mml:mo>
                                             </mml:mrow>
