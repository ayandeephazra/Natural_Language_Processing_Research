                                       </mml:mrow>
                                    </mml:msup>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>±</mml:mo>
                                 </mml:mrow>
                              </mml:msubsup>
                              <mml:mo>=</mml:mo>
                              <mml:mstyle displaystyle="true">
                                 <mml:munder>
                                    <mml:mrow>
                                       <mml:mo>∑</mml:mo>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>i</mml:mi>
                                       <mml:mo>∈</mml:mo>
