                                       <mml:mrow>
                                          <mml:mi>V</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi mathvariant="italic">BZ</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msubsup>
                                 </mml:mrow>
                              </mml:mfrac>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>P</mml:mi>
                                 </mml:mrow>
