                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msqrt>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mrow>
                                                <mml:mi>M</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi>i</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:msub>
                                             <mml:mrow>
                                                <mml:mi>M</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi>j</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:msub>
                                             <mml:mrow>
                                                <mml:mi>M</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi>k</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:msqrt>
                                 </mml:mrow>
                              </mml:mfrac>
                              <mml:mo>,</mml:mo>
