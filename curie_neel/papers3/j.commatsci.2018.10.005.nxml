                                       <mml:mi mathvariant="italic">jk</mml:mi>
                                    </mml:mrow>
                                 </mml:munder>
                              </mml:mstyle>
                              <mml:mstyle displaystyle="true">
                                 <mml:munder>
                                    <mml:mrow>
                                       <mml:mo>∑</mml:mo>
                                    </mml:mrow>
