                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                     <ce:display>
                        <ce:formula id="fd22">
                           <ce:label>(22)</ce:label>
                           <mml:math altimg="si72.svg" alttext="Equation 22.">
                              <mml:mrow>
                                 <mml:msubsup>
                                    <mml:mi>ω</mml:mi>
                                    <mml:mo linebreak="badbreak">+</mml:mo>
                                    <mml:mn>2</mml:mn>
                                 </mml:msubsup>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:msubsup>
                                    <mml:mi>ω</mml:mi>
                                    <mml:mi>L</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msubsup>
                                 <mml:mo linebreak="goodbreak">−</mml:mo>
                                 <mml:msubsup>
                                    <mml:mi>v</mml:mi>
                                    <mml:mrow>
                                       <mml:mi>L</mml:mi>
                                       <mml:mi>O</mml:mi>
                                    </mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:msubsup>
                                 <mml:msup>
                                    <mml:mi>q</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msup>
                                 <mml:mo>,</mml:mo>
                                 <mml:mspace width="1em"/>
                                 <mml:msubsup>
                                    <mml:mi>ω</mml:mi>
                                    <mml:mo linebreak="badbreak">−</mml:mo>
                                    <mml:mn>2</mml:mn>
                                 </mml:msubsup>
                                 <mml:mo linebreak="goodbreak">=</mml:mo>
                                 <mml:msubsup>
                                    <mml:mi>v</mml:mi>
                                    <mml:mrow>
                                       <mml:mi>L</mml:mi>
                                       <mml:mi>A</mml:mi>
                                    </mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:msubsup>
                                 <mml:msup>
                                    <mml:mi>q</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msup>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                     <ce:display>
                        <ce:formula id="fd23">
                           <ce:label>(23)</ce:label>
                           <mml:math altimg="si73.svg" alttext="Equation 23.">
                              <mml:mrow>
                                 <mml:msubsup>
                                    <mml:mi>v</mml:mi>
                                    <mml:mrow>
                                       <mml:mi>L</mml:mi>
                                       <mml:mi>O</mml:mi>
                                    </mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:msubsup>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:msubsup>
                                          <mml:mi>c</mml:mi>
                                          <mml:mn>11</mml:mn>
                                          <mml:mrow>
                                             <mml:mi>o</mml:mi>
                                             <mml:mi>p</mml:mi>
                                          </mml:mrow>
                                       </mml:msubsup>
