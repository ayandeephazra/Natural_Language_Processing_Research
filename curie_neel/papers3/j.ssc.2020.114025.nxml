                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:msub>
                                          <mml:mi>δ</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>σ</mml:mi>
                                             <mml:mi>τ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">]</mml:mo>
                                 </mml:mrow>
                                 <mml:msubsup>
                                    <mml:mi>u</mml:mi>
                                    <mml:mrow>
                                       <mml:mi>β</mml:mi>
                                       <mml:mo>,</mml:mo>
                                       <mml:mi>σ</mml:mi>
                                       <mml:mi>τ</mml:mi>
                                    </mml:mrow>
                                    <mml:mi>A</mml:mi>
                                 </mml:msubsup>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                     <ce:display>
                        <ce:formula id="fd2">
                           <ce:label>(2)</ce:label>
                           <mml:math altimg="si10.svg" alttext="Equation 2.">
                              <mml:mrow>
                                 <mml:mo>−</mml:mo>
                                 <mml:mn>2</mml:mn>
                                 <mml:msup>
                                    <mml:mi>a</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msup>
                                 <mml:msup>
                                    <mml:mi>β</mml:mi>
                                    <mml:mi>A</mml:mi>
                                 </mml:msup>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>δ</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:mi>σ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:msub>
                                          <mml:mi>δ</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>β</mml:mi>
                                             <mml:mi>τ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:msub>
                                          <mml:mi>δ</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:mi>τ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:msub>
                                          <mml:mi>δ</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>β</mml:mi>
                                             <mml:mi>σ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:msubsup>
                                    <mml:mi>u</mml:mi>
                                    <mml:mrow>
                                       <mml:mi>β</mml:mi>
                                       <mml:mo>,</mml:mo>
                                       <mml:mi>σ</mml:mi>
                                       <mml:mi>τ</mml:mi>
                                    </mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:msubsup>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0060" view="all">
                     <mml:math altimg="si11.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mi>m</mml:mi>
                              <mml:mrow>
                                 <mml:mi>A</mml:mi>
                                 <mml:mo>,</mml:mo>
                                 <mml:mi>B</mml:mi>
                              </mml:mrow>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math> are the masses, assumed to be different; <mml:math altimg="si12.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mi>u</mml:mi>
                              <mml:mrow>
                                 <mml:mi>A</mml:mi>
                                 <mml:mo>,</mml:mo>
                                 <mml:mi>B</mml:mi>
                              </mml:mrow>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math> are the displacements from their equilibrium positions, and u is the optical displacement, <mml:math altimg="si13.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mi>u</mml:mi>
                              <mml:mi>A</mml:mi>
                           </mml:msup>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">−</mml:mo>
                           <mml:msup>
                              <mml:mi>u</mml:mi>
                              <mml:mi>B</mml:mi>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math>; 4a is the length of the side of the periodic lattice cube; <mml:math altimg="si14.svg">
                        <mml:mrow>
                           <mml:mi>β</mml:mi>
                        </mml:mrow>
                     </mml:math> is the average bond-bending force equal to <mml:math altimg="si15.svg">
                        <mml:mrow>
                           <mml:mn>1</mml:mn>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">/</mml:mo>
                           <mml:mn>2</mml:mn>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mrow>
                                 <mml:msup>
                                    <mml:mi>β</mml:mi>
                                    <mml:mi>A</mml:mi>
                                 </mml:msup>
                                 <mml:mo linebreak="badbreak">+</mml:mo>
                                 <mml:msup>
                                    <mml:mi>β</mml:mi>
                                    <mml:mi>B</mml:mi>
                                 </mml:msup>
                              </mml:mrow>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math>. The equations are succinctly presented in tensor notation, with the comma denoting differentiation, plus the sum rule for repeated subscripts. <mml:math altimg="si16.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mtext>Δ</mml:mtext>
                              <mml:mrow>
                                 <mml:mi>α</mml:mi>
                                 <mml:mi>β</mml:mi>
                                 <mml:mi>σ</mml:mi>
                                 <mml:mi>τ</mml:mi>
                              </mml:mrow>
                           </mml:msub>
                        </mml:mrow>
                     </mml:math> is an operator, unity if the subscripts come in matching pairs, zero otherwise; <mml:math altimg="si17.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mi>e</mml:mi>
                              <mml:mrow>
                                 <mml:mi>α</mml:mi>
                                 <mml:mi>β</mml:mi>
                                 <mml:mi>γ</mml:mi>
                              </mml:mrow>
                           </mml:msub>
                        </mml:mrow>
                     </mml:math> is the permutation operator, unity if the subscripts are all different and in cyclic order, −1 if the subscripts are all different but in anticyclic order, zero otherwise. Here we need only the modulus, as indicated.</ce:para>
                  <ce:para id="p0065" view="all">Of course, in the diamond lattice the two masses are equal. They are taken to be unequal here in order to accommodate the case of the zinc blende lattice.</ce:para>
                  <ce:para id="p0070" view="all">We convert to the equations of motion for acoustic displacements:<mml:math altimg="si18.svg">
                        <mml:mrow>
                           <mml:mi>U</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:msup>
                              <mml:mi>r</mml:mi>
                              <mml:mi>A</mml:mi>
                           </mml:msup>
                           <mml:msup>
                              <mml:mi>u</mml:mi>
                              <mml:mi>A</mml:mi>
                           </mml:msup>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">+</mml:mo>
                           <mml:msup>
                              <mml:mi>r</mml:mi>
                              <mml:mi>B</mml:mi>
                           </mml:msup>
                           <mml:msup>
                              <mml:mi>u</mml:mi>
                              <mml:mi>B</mml:mi>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math> where <mml:math altimg="si19.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mi>r</mml:mi>
                              <mml:mrow>
                                 <mml:mi>A</mml:mi>
                                 <mml:mo>,</mml:mo>
                                 <mml:mi>B</mml:mi>
                              </mml:mrow>
                           </mml:msup>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mfrac>
                              <mml:mrow>
                                 <mml:msup>
                                    <mml:mi>m</mml:mi>
                                    <mml:mrow>
                                       <mml:mi>A</mml:mi>
                                       <mml:mo>,</mml:mo>
                                       <mml:mi>B</mml:mi>
                                    </mml:mrow>
                                 </mml:msup>
                              </mml:mrow>
                              <mml:mi>M</mml:mi>
                           </mml:mfrac>
                           <mml:mspace width="0.25em"/>
                           <mml:mo>,</mml:mo>
                        </mml:mrow>
                     </mml:math> and by adding eqs. <ce:cross-refs id="crosrefs0020" refid="fd1 fd2">(1) and (2</ce:cross-refs>);<ce:display>
                        <ce:formula id="ufd5">
                           <mml:math altimg="si20.svg">
                              <mml:mrow>
                                 <mml:msup>
                                    <mml:mi>ω</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msup>
                                 <mml:mi>M</mml:mi>
                                 <mml:msub>
                                    <mml:mi>U</mml:mi>
                                    <mml:mi>α</mml:mi>
                                 </mml:msub>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mn>4</mml:mn>
                                 <mml:mi>a</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mi>α</mml:mi>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mi>β</mml:mi>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">|</mml:mo>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>e</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:mi>β</mml:mi>
                                             <mml:mi>γ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">|</mml:mo>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mi>u</mml:mi>
                                    <mml:mrow>
                                       <mml:mi>β</mml:mi>
                                       <mml:mo>,</mml:mo>
                                       <mml:mi>γ</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                     <ce:display>
                        <ce:formula id="ufd6">
                           <mml:math altimg="si21.svg">
                              <mml:mrow>
                                 <mml:mo>−</mml:mo>
                                 <mml:mn>2</mml:mn>
                                 <mml:msup>
                                    <mml:mi>a</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msup>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">[</mml:mo>
                                    <mml:mrow>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:mo linebreak="badbreak">−</mml:mo>
                                             <mml:mi>β</mml:mi>
                                          </mml:mrow>
                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                       <mml:msub>
                                          <mml:mtext>Δ</mml:mtext>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:mi>β</mml:mi>
                                             <mml:mi>σ</mml:mi>
                                             <mml:mi>τ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:mn>2</mml:mn>
                                       <mml:mi>β</mml:mi>
                                       <mml:msub>
                                          <mml:mi>δ</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:mi>β</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:msub>
                                          <mml:mi>δ</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>σ</mml:mi>
                                             <mml:mi>τ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">]</mml:mo>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:msubsup>
                                          <mml:mi>u</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>β</mml:mi>
                                             <mml:mo>,</mml:mo>
                                             <mml:mi>σ</mml:mi>
                                             <mml:mi>τ</mml:mi>
                                          </mml:mrow>
                                          <mml:mi>A</mml:mi>
                                       </mml:msubsup>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:msubsup>
                                          <mml:mi>u</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>β</mml:mi>
                                             <mml:mo>,</mml:mo>
                                             <mml:mi>σ</mml:mi>
                                             <mml:mi>τ</mml:mi>
                                          </mml:mrow>
                                          <mml:mi>B</mml:mi>
                                       </mml:msubsup>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                     <ce:display>
                        <ce:formula id="fd3">
                           <ce:label>(3)</ce:label>
                           <mml:math altimg="si22.svg" alttext="Equation 3.">
                              <mml:mrow>
                                 <mml:mo>−</mml:mo>
                                 <mml:mn>2</mml:mn>
                                 <mml:msup>
                                    <mml:mi>a</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msup>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>δ</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:mi>σ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:msub>
                                          <mml:mi>δ</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>β</mml:mi>
                                             <mml:mi>τ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:msub>
                                          <mml:mi>δ</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:mi>τ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:msub>
                                          <mml:mi>δ</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>β</mml:mi>
                                             <mml:mi>σ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:msup>
                                          <mml:mi>β</mml:mi>
                                          <mml:mi>B</mml:mi>
                                       </mml:msup>
                                       <mml:msubsup>
                                          <mml:mi>u</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>β</mml:mi>
                                             <mml:mo>,</mml:mo>
                                             <mml:mi>σ</mml:mi>
                                             <mml:mi>τ</mml:mi>
                                          </mml:mrow>
                                          <mml:mi>A</mml:mi>
                                       </mml:msubsup>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:msup>
                                          <mml:mi>β</mml:mi>
                                          <mml:mi>A</mml:mi>
                                       </mml:msup>
                                       <mml:msubsup>
                                          <mml:mi>u</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>β</mml:mi>
                                             <mml:mo>,</mml:mo>
                                             <mml:mi>σ</mml:mi>
                                             <mml:mi>τ</mml:mi>
                                          </mml:mrow>
                                          <mml:mi>B</mml:mi>
                                       </mml:msubsup>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0075" view="all">The equation of motion for the optical mode is obtained by multiplying eq. <ce:cross-ref id="crosref0070" refid="fd1">(1)</ce:cross-ref> by <mml:math altimg="si23.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mi>r</mml:mi>
                              <mml:mi>B</mml:mi>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math> and eq.<ce:cross-ref id="crosref0075" refid="fd2">(2)</ce:cross-ref> by <mml:math altimg="si24.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mi>r</mml:mi>
                              <mml:mi>A</mml:mi>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math> and subtracting:<ce:display>
                        <ce:formula id="ufd7">
                           <mml:math altimg="si25.svg">
                              <mml:mrow>
                                 <mml:msup>
                                    <mml:mi>ω</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msup>
                                 <mml:mi>μ</mml:mi>
                                 <mml:msub>
                                    <mml:mi>u</mml:mi>
                                    <mml:mi>α</mml:mi>
                                 </mml:msub>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mn>4</mml:mn>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mi>α</mml:mi>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:mi>β</mml:mi>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mi>u</mml:mi>
                                    <mml:mi>α</mml:mi>
                                 </mml:msub>
                                 <mml:mo linebreak="goodbreak">−</mml:mo>
                                 <mml:mn>4</mml:mn>
                                 <mml:mi>a</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mi>α</mml:mi>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mi>β</mml:mi>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">|</mml:mo>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>e</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:mi>β</mml:mi>
                                             <mml:mi>γ</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">|</mml:mo>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:msup>
                                          <mml:mi>r</mml:mi>
                                          <mml:mi>A</mml:mi>
                                       </mml:msup>
                                       <mml:msubsup>
                                          <mml:mi>u</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>β</mml:mi>
                                             <mml:mo>,</mml:mo>
                                             <mml:mi>γ</mml:mi>
                                          </mml:mrow>
                                          <mml:mi>A</mml:mi>
                                       </mml:msubsup>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:msup>
                                          <mml:mi>r</mml:mi>
                                          <mml:mi>B</mml:mi>
                                       </mml:msup>
                                       <mml:msubsup>
                                          <mml:mi>u</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>β</mml:mi>
                                             <mml:mo>,</mml:mo>
                                             <mml:mi>γ</mml:mi>
                                          </mml:mrow>
                                          <mml:mi>B</mml:mi>
                                       </mml:msubsup>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                              </mml:mrow>
