                           <mml:math altimg="si71.svg" alttext="Equation 21.">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>γ</mml:mi>
                                    <mml:mrow>
                                       <mml:mi>o</mml:mi>
                                       <mml:mi>p</mml:mi>
                                       <mml:mi>t</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>c</mml:mi>
                                          <mml:mn>11</mml:mn>
                                       </mml:msub>
                                       <mml:mi>r</mml:mi>
