                                       <mml:mi>a</mml:mi>
                                    </mml:mrow>
                                    <mml:mi>ρ</mml:mi>
                                 </mml:mfrac>
                                 <mml:mo>,</mml:mo>
                                 <mml:mspace width="1em"/>
                                 <mml:msub>
                                    <mml:mi>γ</mml:mi>
                                    <mml:mrow>
                                       <mml:mi>a</mml:mi>
                                       <mml:mi>c</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo linebreak="goodbreak">=</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>c</mml:mi>
                                          <mml:mn>11</mml:mn>
                                       </mml:msub>
                                       <mml:mi>r</mml:mi>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mtext>Δ</mml:mtext>
                                       <mml:mi>β</mml:mi>
                                       <mml:mo linebreak="badbreak">/</mml:mo>
                                       <mml:mn>4</mml:mn>
                                       <mml:mi>a</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mover accent="true">
                                          <mml:mi>ρ</mml:mi>
                                          <mml:mo>‾</mml:mo>
                                       </mml:mover>
