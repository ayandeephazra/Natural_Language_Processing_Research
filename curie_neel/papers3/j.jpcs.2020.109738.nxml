                                       <mml:mrow>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">[</mml:mo>
                                             <mml:mrow>
                                                <mml:mfrac>
                                                   <mml:mi mathvariant="normal">c</mml:mi>
                                                   <mml:mi mathvariant="normal">t</mml:mi>
                                                </mml:mfrac>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mrow>
                                                      <mml:mfrac>
                                                         <mml:mn mathvariant="normal">1</mml:mn>
                                                         <mml:mrow>
                                                            <mml:msubsup>
                                                               <mml:mi mathvariant="normal">ε</mml:mi>
                                                               <mml:mo>⊥</mml:mo>
                                                               <mml:mi mathvariant="normal">s</mml:mi>
                                                            </mml:msubsup>
