                                             <mml:mrow>
                                                <mml:mn>2</mml:mn>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mo linebreak="badbreak" linebreakstyle="after">−</mml:mo>
                                          <mml:mi>s</mml:mi>
                                          <mml:mi>i</mml:mi>
                                          <mml:mi>n</mml:mi>
                                          <mml:msub>
                                             <mml:mrow>
                                                <mml:mi>θ</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mn>1</mml:mn>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mo stretchy="false">)</mml:mo>
                                       </mml:mrow>
                                    </mml:mfrac>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display> n are integers starting from zero and <mml:math altimg="si47.svg">
                           <mml:mo stretchy="false">(</mml:mo>
