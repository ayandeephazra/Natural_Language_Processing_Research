                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>L</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>k</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mo linebreak="goodbreak" linebreakstyle="after">&lt;</mml:mo>
                                    <mml:msubsup>
                                       <mml:mrow>
                                          <mml:mi>ϵ</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>g</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo linebreak="goodbreak" linebreakstyle="after">&gt;</mml:mo>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display> The essential relation of the Warren–Averbach evaluation, then written as:<ce:display>
                           <ce:formula id="fm0090">
                              <ce:label>(9)</ce:label>
                              <mml:math altimg="si43.svg">
                                 <mml:mrow>
                                    <mml:mi mathvariant="normal">ln</mml:mi>
                                    <mml:mo>⁡</mml:mo>
                                    <mml:mi>A</mml:mi>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mi>L</mml:mi>
                                    <mml:mo stretchy="false">)</mml:mo>
                                    <mml:mo linebreak="badbreak" linebreakstyle="after">=</mml:mo>
                                    <mml:mi mathvariant="normal">ln</mml:mi>
                                    <mml:mo>⁡</mml:mo>
                                    <mml:msubsup>
                                       <mml:mrow>
                                          <mml:mi>A</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>L</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>S</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo linebreak="goodbreak" linebreakstyle="after">−</mml:mo>
                                    <mml:mn>2</mml:mn>
                                    <mml:msup>
                                       <mml:mrow>
