                                    <mml:mrow>
                                       <mml:mo>(</mml:mo>
                                       <mml:mrow>
                                          <mml:mo>−</mml:mo>
                                          <mml:mfrac>
                                             <mml:mtext>λ</mml:mtext>
                                             <mml:mrow>
                                                <mml:mn>4</mml:mn>
                                                <mml:msub>
                                                   <mml:mtext>k</mml:mtext>
                                                   <mml:mi mathvariant="bold">B</mml:mi>
                                                </mml:msub>
