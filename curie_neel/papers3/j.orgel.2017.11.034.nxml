                                             <mml:mi>cos</mml:mi>
                                          </mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                       <mml:mrow>
                                          <mml:mo>(</mml:mo>
                                          <mml:mn>47.7</mml:mn>
                                          <mml:mo>°</mml:mo>
                                          <mml:mo>+</mml:mo>
                                          <mml:mi mathvariant="normal">φ</mml:mi>
                                          <mml:mo>)</mml:mo>
                                       </mml:mrow>
                                       <mml:mo>+</mml:mo>
                                       <mml:mn>4.80</mml:mn>
                                       <mml:mspace width="0.25em"/>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mi>cos</mml:mi>
                                          </mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                       <mml:mrow>
                                          <mml:mo>(</mml:mo>
                                          <mml:mn>54.2</mml:mn>
                                          <mml:mo>°</mml:mo>
                                          <mml:mo>−</mml:mo>
                                          <mml:mi mathvariant="normal">φ</mml:mi>
                                          <mml:mo>)</mml:mo>
                                       </mml:mrow>
                                       <mml:mo>+</mml:mo>
                                       <mml:mn>0.16</mml:mn>
                                       <mml:mspace width="0.25em"/>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mi>cos</mml:mi>
                                          </mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                       <mml:mi mathvariant="normal">φ</mml:mi>
                                    </mml:mrow>
                                 </mml:math>
                              </ce:formula>
                           </ce:display>and<ce:display>
                              <ce:formula id="fd14">
                                 <ce:label>(14)</ce:label>
                                 <mml:math altimg="si14.gif" overflow="scroll">
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mtext>μ</mml:mtext>
                                          <mml:mi mathvariant="normal">φ</mml:mi>
                                       </mml:msub>
                                       <mml:mo>=</mml:mo>
                                       <mml:mn>5.381</mml:mn>
                                       <mml:mspace width="0.25em"/>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mi>cos</mml:mi>
                                          </mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                       <mml:mrow>
                                          <mml:mo>(</mml:mo>
                                          <mml:mn>41.45</mml:mn>
                                          <mml:mo>°</mml:mo>
                                          <mml:mo>−</mml:mo>
                                          <mml:mi mathvariant="normal">φ</mml:mi>
                                          <mml:mo>)</mml:mo>
                                       </mml:mrow>
                                       <mml:mo>+</mml:mo>
                                       <mml:mn>0.18</mml:mn>
                                       <mml:mspace width="0.25em"/>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mi>cos</mml:mi>
                                          </mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                       <mml:mrow>
                                          <mml:mo>(</mml:mo>
                                          <mml:mn>88.76</mml:mn>
                                          <mml:mo>°</mml:mo>
                                          <mml:mo>−</mml:mo>
                                          <mml:mi mathvariant="normal">φ</mml:mi>
                                          <mml:mo>)</mml:mo>
                                       </mml:mrow>
                                       <mml:mo>+</mml:mo>
                                       <mml:mn>0.881</mml:mn>
                                       <mml:mspace width="0.25em"/>
