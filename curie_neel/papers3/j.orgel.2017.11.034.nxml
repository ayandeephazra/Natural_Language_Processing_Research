                                                            <mml:mtext>λk</mml:mtext>
                                                         </mml:mrow>
                                                         <mml:mtext>B</mml:mtext>
                                                      </mml:msub>
                                                      <mml:mtext>T</mml:mtext>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                             </mml:mrow>
                                             <mml:mo>)</mml:mo>
                                          </mml:mrow>
                                       </mml:mrow>
