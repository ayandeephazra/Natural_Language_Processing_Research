                  <ce:text>Thickness-controlled layer separation</ce:text>
               </ce:keyword>
               <ce:keyword id="k0010">
                  <ce:text>Spalling technique</ce:text>
               </ce:keyword>
               <ce:keyword id="k0015">
                  <ce:text>Analytic model</ce:text>
               </ce:keyword>
               <ce:keyword id="k0020">
                  <ce:text>Thermodynamic equilibrium condition</ce:text>
               </ce:keyword>
               <ce:keyword id="k0025">
                  <ce:text>Crack initiation angle</ce:text>
               </ce:keyword>
            </ce:keywords>
