                                                <mml:mi mathvariant="normal">ε</mml:mi>
                                                <mml:mtext>h</mml:mtext>
                                                <mml:mtext>c</mml:mtext>
                                             </mml:msubsup>
                                             <mml:mo>–</mml:mo>
                                             <mml:msubsup>
                                                <mml:mi mathvariant="normal">ε</mml:mi>
                                                <mml:mtext>h</mml:mtext>
                                                <mml:mtext>a</mml:mtext>
                                             </mml:msubsup>
                                          </mml:mrow>
                                          <mml:mo>)</mml:mo>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mo>/</mml:mo>
                                    <mml:mn>2</mml:mn>
