                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>+</mml:mo>
                                 <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:mrow>
                                       <mml:mn>1</mml:mn>
                                       <mml:mo>−</mml:mo>
                                       <mml:mtext>x</mml:mtext>
                                    </mml:mrow>
                                    <mml:mo>)</mml:mo>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mtext>a</mml:mtext>
                                    <mml:mrow>
                                       <mml:mtext>Ge</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>+</mml:mo>
                                 <mml:mtext>bx</mml:mtext>
                                 <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:mrow>
                                       <mml:mn>1</mml:mn>
                                       <mml:mo>−</mml:mo>
                                       <mml:mtext>x</mml:mtext>
