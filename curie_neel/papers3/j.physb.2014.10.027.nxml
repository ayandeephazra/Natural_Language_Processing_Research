                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mn>1</mml:mn>
                              <mml:mo>/</mml:mo>
                              <mml:mi>λ</mml:mi>
                              <mml:mo stretchy="false">)</mml:mo>
