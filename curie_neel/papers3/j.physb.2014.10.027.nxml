                                       <mml:mrow>
                                          <mml:mi>n</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo stretchy="false">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo stretchy="true">)</mml:mo>
                              </mml:mrow>
                           </mml:mrow>
                           <mml:mo stretchy="true">]</mml:mo>
                        </mml:mrow>
                     </mml:math>; <mml:math altimg="si0035.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>I</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>2</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>=</mml:mo>
                        <mml:msub>
                           <mml:mrow>
