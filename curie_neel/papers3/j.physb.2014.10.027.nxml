                        </mml:msub>
                        <mml:mo>=</mml:mo>
                        <mml:mrow>
                           <mml:mo stretchy="true">(</mml:mo>
                           <mml:mrow>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mn>1</mml:mn>
                              <mml:mo>/</mml:mo>
                              <mml:mi>λ</mml:mi>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>+</mml:mo>
                              <mml:mspace width=".25em"/>
