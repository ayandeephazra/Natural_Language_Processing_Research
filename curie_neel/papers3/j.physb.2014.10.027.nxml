                           </mml:mrow>
                           <mml:mo stretchy="true">)</mml:mo>
                        </mml:mrow>
                        <mml:mi>ln</mml:mi>
                        <mml:mrow>
                           <mml:mo stretchy="true">[</mml:mo>
                           <mml:mrow>
                              <mml:mn>1</mml:mn>
                              <mml:mo>+</mml:mo>
                              <mml:mi>exp</mml:mi>
                              <mml:mrow>
                                 <mml:mo stretchy="true">(</mml:mo>
                                 <mml:mrow>
                                    <mml:mi>β</mml:mi>
                                    <mml:mo>−</mml:mo>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo>/</mml:mo>
                                    <mml:msub>
                                       <mml:mrow>
